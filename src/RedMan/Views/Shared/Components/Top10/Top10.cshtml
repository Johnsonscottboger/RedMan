@*用户积分排行榜-显示10条*@

@model IEnumerable<Web.Model.Entities.User>
<ol>
    @foreach(var item in Model) {
        <li>
            <span class="top_score">@item.Score</span>
            <span class="user_name"><a href="@Url.Action("Detail","User",new { username=item.Name})">@item.Name</a></span>
        </li>
    }
</ol>